{
  "name": "wysiwyg-testing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "jest --coverage",
    "eject": "react-scripts eject",
    "bounce": "rm -rf node_modules && yarn install",
    "deadcode": "node scripts/deadcode.mjs",
    "lint": "eslint --cache --ext .js,.ts,.tsx src",
    "prepare": "husky install",
    "ts-check": "yarn tsc",
    "generate:scss-types": "typed-scss-modules \"src/**/*.module.scss\" --exportType default --nameFormat all --includePaths src"
  },
  "dependencies": {
    "@lexical/clipboard": "^0.11.3",
    "@lexical/code": "^0.11.3",
    "@lexical/file": "^0.11.3",
    "@lexical/hashtag": "^0.11.3",
    "@lexical/html": "^0.11.3",
    "@lexical/link": "^0.11.3",
    "@lexical/list": "^0.11.3",
    "@lexical/mark": "^0.11.3",
    "@lexical/markdown": "^0.11.3",
    "@lexical/overflow": "^0.11.3",
    "@lexical/plain-text": "^0.11.3",
    "@lexical/react": "^0.11.3",
    "@lexical/rich-text": "^0.11.3",
    "@lexical/selection": "^0.11.3",
    "@lexical/table": "^0.11.3",
    "@lexical/utils": "^0.11.3",
    "@lexical/yjs": "^0.11.3",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/node": "^20.4.5",
    "@types/react": "^18.2.18",
    "katex": "^0.16.8",
    "lexical": "^0.11.3",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "prettier": "^3.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1",
    "typescript": "^5.1.6",
    "web-vitals": "^2.1.4",
    "y-websocket": "^1.5.0",
    "yjs": "^13.6.7"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/katex": "^0.16.2",
    "@types/lodash-es": "^4.17.8",
    "@types/prettier": "3.0.0",
    "chalk": "^5.3.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-typescript-sort-keys": "^2.3.0",
    "husky": "^8.0.3",
    "sass": "^1.65.1",
    "typed-scss-modules": "^7.1.4"
  }
}
